{
    "name": "caciobanu/guzzle-bundle",
    "type": "symfony-bundle",
    "description": "Integration bundle for guzzle with Symfony.",
    "homepage": "https://github.com/caciobanu/guzzle-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":  "Catalin Ciobanu",
            "email": "caciobanu@gmail.com"
        }
    ],
    "require": {
        "php":                      "^7.1",
        "guzzlehttp/guzzle":        "^6.0",
        "symfony/framework-bundle": "^4.0",
        "psr/log":                  "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "symfony/yaml": "^4.0",
        "phpstan/phpstan-shim": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "suggest" : {
        "symfony/monolog-bundle": "If you want to enable logging and don't want to implement your own 'logger' service."
    },
    "autoload": {
        "psr-4": {
            "Caciobanu\\Symfony\\GuzzleBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caciobanu\\Symfony\\GuzzleBundle\\Tests\\": "tests"
        }
    }
}
